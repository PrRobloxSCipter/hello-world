local L = false
local function ClearChat()
  coroutine.wrap(function()
    while task.wait(1) do
      if L then
        break
      end
      local str2 = string.rep("\n\r\n",66) .. "." .. string.rep("\n\r\n",66)
      game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(str2..".")
    end
  end)()
end

